        "@types/jest": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "@types/react-redux": "latest",
        "@types/redux-actions": "latest",
        "@types/redux-immutable-state-invariant": "latest",
        "@types/stylus": "latest",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "babel-loader": "latest",
        "babel-plugin-stylus-compiler": "latest",
        "css-loader": "latest",
        "eslint": "latest",
        "eslint-config-airbnb-base": "latest",
        "eslint-config-airbnb-typescript": "latest",
        "eslint-config-prettier": "latest",
        "eslint-plugin-flowtype": "latest",
        "eslint-plugin-import": "latest",
        "eslint-plugin-jasmine": "latest",
        "eslint-plugin-jest": "latest",
        "eslint-plugin-jsx-a11y": "latest",
        "eslint-plugin-lodash": "latest",
        "eslint-plugin-prettier": "latest",
        "eslint-plugin-react": "latest",
        "eslint-plugin-react-hooks": "latest",

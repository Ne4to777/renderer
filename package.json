{
    "name": "renderer",
    "version": "0.0.1",
    "description": "Renderer",
    "main": "dist/index",
    "scripts": {
        "serve": "npx webpack serve --config ./configs/webpack.config.js",
        "build:webpack": "npm run remove:dist && npx webpack build --config ./configs/webpack.config.js",
        "build:ts": "npm run remove:build && npx tsc --build tsconfig.json",
        "copy:files": "cp package.json package-lock.json README.md build/",
        "test": "npx jest --config ./configs/jest.config.js",
        "test:watch": "npm run test -- --watchAll",
        "test:coverage": "npx jest --coverage",
        "lint": "npx eslint --ext .ts --ext .tsx src/",
        "lint:fix": "npm run lint -- --fix",
        "lint:perfomance": "TIMING=1 npx eslint ./",
        "publish": "npm run build && npm publish build/ --public",
        "clean:ts": "npm run remove:build && npm run remove:coverage",
        "remove:dist": "rm -rf dist",
        "remove:build": "rm -rf build",
        "remove:coverage": "rm -rf coverage",
        "revert": "rm -rf node_modules && rm -rf package-lock.json",
        "reinstall": "npm run revert && npm i"
    },
    "devDependencies": {
        "@babel/core": "latest",
        "@babel/plugin-proposal-class-properties": "latest",
        "@babel/preset-env": "latest",
        "@babel/preset-react": "latest",
